{
    "version": 2,
    "public": true,
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Cross-Origin-Resource-Policy",
            "value": "cross-origin"
          },
          {
            "key": "Cross-Origin-Embedder-Policy",
            "value": "require-corp"
          },
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin"
          }
        ]
      },
      {
        "source": "/models/(.*).glb",
        "headers": [
          {
            "key": "Content-Type",
            "value": "model/gltf-binary"
          }
        ]
      },
      {
        "source": "/models/(.*).usdz",
        "headers": [
          {
            "key": "Content-Type",
            "value": "model/vnd.usdz+zip"
          }
        ]
      }
    ]
  }